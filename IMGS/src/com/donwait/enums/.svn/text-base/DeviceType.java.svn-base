package com.donwait.enums;

public enum DeviceType {
	DEVICE_TYPE_IPC(0X01),
	DEVICE_TYPE_NVR(0X02),
	DEVICE_TYPE_DVR(0X04),
	DEVICE_TYPE_DEC(0X08),
	DEVICE_TYPE_DOOR(0X10),
	DEVICE_TYPE_FACE_SVR(0X20),
	DEVICE_TYPE_NVAS_SVR(0X40),
	DEVICE_TYPE_PC_SHOW(0X80),
	DEVICE_TYPE_ANDROID_SHOW(0X100);
	
	private long value;
	private DeviceType(long value){
		this.value = value;
	}
	public long getValue(){
		return this.value;
	}
}
